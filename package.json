  "bin": {
    "lyi-service-dev": "./bin/cli.js"
  },
  "dependencies": {
    "config": "^1.31.0",
    "express": "^4.16.3"
  },
  "files": [
    "/__test__",
    "/bin",
    "/jest.config.js",
    "/app",
    "/config",
    "/package.json",
    "/*.dockerfile",
    "/*.eslintrc.js",
    "/*.prettierrc"
  ],
  "devDependencies": {
    "eslint": "^5.2.0",
    "husky": "^0.14.3",
